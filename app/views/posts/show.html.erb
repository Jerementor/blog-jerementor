<div class="content-box w-clearfix">
  
  <h2><%= @post.title %></h2>
  <h4><%= @post.subtitle %></h4>
  <h5><%= @post.name %></h5>


  <div class="trix-content">  <%= @post.content.html_safe %></div>

</div>

<% if user_signed_in? %>
<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
<% end %>